  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "axios": "^0.18.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.1.0",
    "express": "^4.16.3",

    "react-filter-search": "^1.0.8",
    "react-router-dom": "^5.1.2",

    "express-session": "^1.17.0",
    "if-env": "^1.0.4",
    "mongoose": "^5.3.16",
    "node-fetch": "^2.6.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "serve-static": "^1.14.1",
    "superagent": "^5.1.0",
    "superagent-use": "^0.1.0"
  }
